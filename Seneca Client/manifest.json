{
  "manifest_version": 2,

  "name": "Seneca Client",
  "description": ":)",
  "version": "0.1",
  "permissions": [
        "activeTab",
        "tabs",
        "windows"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
 
 
}
